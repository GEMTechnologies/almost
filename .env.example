# Database Configuration
DATABASE_URL="postgresql://username:password@localhost:5432/granada_os"

# PayPal API Configuration (Required for PayPal payments)
PAYPAL_CLIENT_ID="your_paypal_client_id_here"
PAYPAL_CLIENT_SECRET="your_paypal_client_secret_here"

# PesaPal API Configuration (Required for mobile money payments)
PESAPAL_CONSUMER_KEY="your_pesapal_consumer_key_here"
PESAPAL_CONSUMER_SECRET="your_pesapal_consumer_secret_here"

# Optional: DeepSeek AI API Key (for AI features)
DEEPSEEK_API_KEY="your_deepseek_api_key_here"

# Base URL for callbacks (set in production)
BASE_URL="http://localhost:5000"

# Node Environment
NODE_ENV="development"